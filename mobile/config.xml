<?xml version='1.0' encoding='utf-8'?>
<widget id="com.onemorecode.comibook" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>OneMoreCode</name>
    <description>
	Aplicacion de emprendedores para emprendedores
    </description>
    <author email="wrtfix@gmail.com" href="https://onemorecode-qa.000webhostapp.com/">
        Equipo OneMoreCode
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-inappbrowser" spec="^3.1.0" />
    <engine name="android" spec="^7.0.0" />
</widget>
